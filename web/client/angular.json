{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "dashboard": {
      "root": "projects/applications/dashboard/",
      "sourceRoot": "projects/applications/dashboard/src",
      "projectType": "application",
      "prefix": "gs",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/applications/dashboard",
            "index": "projects/applications/dashboard/src/index.html",
            "main": "projects/applications/dashboard/src/main.ts",
            "polyfills": "projects/applications/dashboard/src/polyfills.ts",
            "tsConfig": "projects/applications/dashboard/tsconfig.app.json",
            "assets": [
              "projects/applications/dashboard/src/favicon.ico",
              "projects/applications/dashboard/src/assets"
            ],
            "styles": [
              "projects/applications/dashboard/src/assets/scss/fontawesome/scss/fontawesome.scss",
              "projects/applications/dashboard/src/assets/scss/fontawesome/scss/brands.scss",
              "projects/applications/dashboard/src/assets/scss/fontawesome/scss/regular.scss",
              "projects/applications/dashboard/src/assets/scss/fontawesome/scss/solid.scss",
              "projects/applications/dashboard/src/assets/scss/fontawesome/scss/v4-shims.scss",
              "projects/applications/dashboard/src/assets/scss/bootstrap.scss",
              "projects/applications/dashboard/src/styles.scss",
              "./node_modules/ngx-ui-switch/ui-switch.component.css"
            ],
            "scripts": [
              "projects/applications/dashboard/src/assets/js/jquery/jquery-3.3.1.min.js",
              "projects/applications/dashboard/src/assets/js/popper/popper.min.js",
              "projects/applications/dashboard/src/assets/js/bootstrap/bootstrap.min.js"
            ],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/applications/dashboard/src/environments/environment.ts",
                  "with": "projects/applications/dashboard/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "dashboard:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "dashboard:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dashboard:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/applications/dashboard/src/test.ts",
            "polyfills": "projects/applications/dashboard/src/polyfills.ts",
            "tsConfig": "projects/applications/dashboard/tsconfig.spec.json",
            "karmaConfig": "projects/applications/dashboard/karma.conf.js",
            "styles": [
              "projects/applications/dashboard/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "projects/applications/dashboard/src/favicon.ico",
              "projects/applications/dashboard/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/applications/dashboard/tsconfig.app.json",
              "projects/applications/dashboard/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "dashboard-e2e": {
      "root": "projects/applications/dashboard-e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/applications/dashboard-e2e/protractor.conf.js",
            "devServerTarget": "dashboard:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dashboard:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/applications/dashboard-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "gs-common": {
      "root": "projects/libraries/gs-common",
      "sourceRoot": "projects/libraries/gs-common/src",
      "projectType": "library",
      "prefix": "gs-common",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/libraries/gs-common/tsconfig.lib.json",
            "project": "projects/libraries/gs-common/ng-package.json"
          }            
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/libraries/gs-common/src/test.ts",
            "tsConfig": "projects/libraries/gs-common/tsconfig.spec.json",
            "karmaConfig": "projects/libraries/gs-common/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/libraries/gs-common/tsconfig.lib.json",
              "projects/libraries/gs-common/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "gs-core": {
      "root": "projects/libraries/gs-core",
      "sourceRoot": "projects/libraries/gs-core/src",
      "projectType": "library",
      "prefix": "gs",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/libraries/gs-core/tsconfig.lib.json",
            "project": "projects/libraries/gs-core/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/libraries/gs-core/src/test.ts",
            "tsConfig": "projects/libraries/gs-core/tsconfig.spec.json",
            "karmaConfig": "projects/libraries/gs-core/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/libraries/gs-core/tsconfig.lib.json",
              "projects/libraries/gs-core/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "dashboard"
}